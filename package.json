{
    "name": "sol-interp",
    "version": "0.2.0",
    "description": "Solidity interpreter",
    "keywords": [],
    "files": [
        "dist"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "sol-interp": "dist/bin/cli.js"
    },
    "type": "commonjs",
    "scripts": {
        "clean": "rm -rf dist/",
        "transpile": "tsc",
        "build-step-parser": "peggy -o src/bin/ast/step_parser_gen.js --dts src/bin/ast/step.pegjs",
        "build": "npm run clean && npm run build-step-parser && npm run transpile",
        "lint": "eslint src/ test/ --ext=ts",
        "lint:fix": "eslint src/ test/ --ext=ts --fix",
        "test": "jest --coverage",
        "prepare": "npm run build"
    },
    "dependencies": {
        "commander": "14.0.2",
        "solc-typed-ast": "^18.2.6",
        "sol-dbg": "0.8.15",
        "@ethereumjs/util": "10.1.0",
        "semver": "^7.6.3",
        "web3-eth-abi": "^4.4.1",
        "src-location": "^1.1.0"
    },
    "devDependencies": {
        "@types/debug": "^4.1.12",
        "fs-extra": "^11.2.0",
        "@types/fs-extra": "^11.0.4",
        "@types/jest": "^29.5.12",
        "@types/lru-cache": "^5.1.1",
        "@types/node": "^22.0.0",
        "@types/semver": "^7.5.8",
        "@typescript-eslint/eslint-plugin": "^7.0.2",
        "@typescript-eslint/parser": "^7.0.2",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "expect": "^29.7.0",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "peggy": "5.0.6"
    },
    "homepage": "https://github.com/d1m0/sol-interp",
    "bugs": "https://github.com/d1m0/sol-interp/issues",
    "repository": "https://github.com/d1m0/sol-interp",
    "license": "MIT"
}